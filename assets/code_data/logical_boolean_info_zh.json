[
  {
    "rule": "^true$",
    "translate": "真",
    "description": "该值将满足条件。"
  },
  {
    "rule": "^false$",
    "translate": "假",
    "description": "该值不满足条件。"
  },
  {
    "rule": "^if$",
    "translate": "如果",
    "description": "使用 如果 开始所有逻辑布尔值，除非只是使用布尔值（真/假）"
  },
  {
    "rule": "^and$",
    "translate": "且",
    "description": "连接符。向列表中添加另一个条件。通过此连接符链接的所有条件都必须满足才能返回真。"
  },
  {
    "rule": "^or$",
    "translate": "或",
    "description": "连接器。向列表中添加另一个条件。只要满足其中一个条件就可以返回真。"
  },
  {
    "rule": "^not$",
    "translate": "非",
    "description": "条件。反转下一个逻辑布尔值的含义。用于设置负面条件。"
  },
  {
    "rule": "^<$",
    "translate": "小于",
    "description": "如果左边的数字较小，则返回真。表示小于下一个值。"
  },
  {
    "rule": "^>$",
    "translate": "大于",
    "description": "如果左边的数字较大，则返回真。表示大于下一个值。"
  },
  {
    "rule": "^<=$",
    "translate": "小于等于",
    "description": "如果左边的数字较小或相等，则返回真。表示小于或等于下一个值。"
  },
  {
    "rule": "^>=$",
    "translate": "大于等于",
    "description": "如果左边的数字较大或相等，则返回真。表示大于或等于下一个值。"
  },
  {
    "rule": "^==$",
    "translate": "等于",
    "description": "如果两边的数字， 单位, 字符串， 布尔值相同则返回真。均值等于下一个值。"
  },
  {
    "rule": "^!=$",
    "translate": "不等于",
    "description": "如果两边的数字， 单位, 字符串， 布尔值不同则返回真。均值不等于下一个值。"
  },
  {
    "rule": "^\\+$",
    "translate": "加",
    "description": "将两个数字相加或连接两个字符串。表示对该值的加法。"
  },
  {
    "rule": "^-$",
    "translate": "减",
    "description": "减去两个数。表示这个值的减法。"
  },
  {
    "rule": "^/$",
    "translate": "除",
    "description": "除以两个数。表示两个值的除法。"
  },
  {
    "rule": "^\\*$",
    "translate": "乘",
    "description": "将两个数相乘。表示对该值的乘法。"
  },
  {
    "rule": "^%$",
    "translate": "取余",
    "description": "除两个数并返回余数。表示值的百分比。"
  },
  {
